  "components": {
    "schemas": {
      "Order": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64",
            "example": 40953
          },
          "shop": {
            "type": "string",
            "example": "Mc Donalds"
          },
          "order": {
            "type": "string",
            "example": "Hamburguesa1, Cocacola"
          },
          "shipDate": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "type": "string",
            "description": "Order Status",
            "example": "Entregado",
            "enum": [
              "Entregado",
              "En Camino",
              "disputado",
              "Solucionado"
            ]
          },
          "Price": {
            "type": "integer",
            "example": 2.3
          },
          "pickAddress": {
            "type": "string",
            "example": "-34.50718, -58.77013"
          },
          "deliveryAddress": {
            "type": "string",
            "example": "-34.5071, -58.770"
          },
          "quarrelDescription": {
            "type": "string",
            "example": "Llegó en mal estado"
          },
          "quarrelPicture": {
            "type": "string",
            "example": "img en base64"
          },
          "reviewDescription": {
            "type": "string",
            "example": "Buen Restaurante"
          },
          "reviewLevel": {
            "type": "integer",
            "example": 5
          },
          "deliverId": {
            "type": "integer",
            "example": 10
          },
          "deliverMoney": {
            "type": "number",
            "example": 0.3
          },
          "shopId": {
            "type": "integer",
            "example": 15
          },
          "shopMoney": {
            "type": "number",
            "example": 2
          },
          "userId": {
            "type": "integer",
            "example": 20
          }
        },
        "xml": {
          "name": "order"
        }
      },
      "Customer": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64",
            "example": 100000
          },
          "username": {
            "type": "string",
            "example": "fehguy"
          },
          "address": {
            "type": "array",
            "xml": {
              "name": "addresses",
              "wrapped": true
            },
            "items": {
              "$ref": "#/components/schemas/Address"
            }
          }
        },
        "xml": {
          "name": "customer"
        }
      },
      "Address": {
        "type": "object",
        "properties": {
          "street": {
            "type": "string",
